{
  "name": "hamus.js",
  "version": "0.3.0",
  "description": "Collections of React Hooks for use with Qlik APIs",
  "author": "QlikTech International AB",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/qlik-oss/hamus.js"
  },
  "scripts": {
    "example": "parcel ./example/index.html",
    "example:cov": "cross-env NODE_ENV=test parcel ./example/index.html",
    "lint": "eslint src example --ext .js*",
    "lint:fix": "eslint src example --ext .js* --fix",
    "test:unit": "jest test/unit",
    "test:e2e": "jest -c test/e2e/jest.config.js test/e2e",
    "preversion": "npm run lint && npm run test:unit && npm run test:e2e"
  },
  "files": [
    "src/"
  ],
  "main": "src/index.js",
  "dependencies": {
    "react-use-promise": "0.2.0"
  },
  "peerDependencies": {
    "picasso.js": ">=0.14.0",
    "picasso-plugin-q": ">=0.14.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "enigma.js": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-transform-async-to-generator": "7.4.4",
    "@babel/plugin-transform-react-jsx": "7.3.0",
    "@babel/plugin-transform-runtime": "7.4.4",
    "@babel/preset-env": "7.4.4",
    "@babel/preset-react": "7.0.0",
    "babel-eslint": "10.0.1",
    "cross-env": "5.2.0",
    "enigma.js": "2.4.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.13.0",
    "jest": "24.7.1",
    "react-hooks-testing-library": "0.5.0",
    "puppeteer": "1.15.0",
    "parcel-bundler": "1.12.3",
    "picasso.js": "0.25.1",
    "picasso-plugin-q": "0.25.1",
    "react": "16.9.0-alpha.0",
    "react-dom": "16.9.0-alpha.0",
    "jest-puppeteer": "4.1.1",
    "coveralls": "3.0.3",
    "react-testing-library": "7.0.0",
    "react-test-renderer": "16.9.0-alpha.0"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageOnlyFrom": {
      "src/use-layout.js": true,
      "src/use-model.js": true,
      "src/use-picasso.js": true,
      "src/render-debouncer.js": true
    }
  }
}
